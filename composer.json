{
    "name": "jobilla/laravel-loco-tms",
    "description": "A Laravel convenience integration for the Loco translation management system",
    "type": "library",
    "require": {
        "ext-json": "*",
        "illuminate/console": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/filesystem": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/translation": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "loco/loco": "^1.0|^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Leo SjoÌˆberg",
            "email": "leo@jobilla.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Jobilla\\Loco\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jobilla\\Loco\\Tests": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jobilla\\Loco\\LocoServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:andvla/loco.git"
        }
    ]
}
